
page {
	background-color: #F8F8FB;
}
.content {
	width: 100%;
	height: 100%;
}
.navigate {
	width: 100%;
	height: 165px;
	background-color: #ffffff;
	background-image: linear-gradient(62deg, #ffffff 9%, #8EC5FC 49%, #E0C3FC 63%);
	position: relative;
}
.colorbg {
	width: 100%;
	height: 165px;
	background-color: #FFFFFF;
	position: absolute;
	top: 0;
	-webkit-filter: blur(100px);
	        filter: blur(100px);
	z-index: 1;
}
.datetime {
	width: 100%;
	height: 100px;
	position: absolute;
	top: 60px;
}
.lefttitle {
	margin-top: 30px;
	float: left;
	margin-left: 30px;
	font-size: 23px;
	font-weight: bold;
}
.rightdatetime {
	margin-top: 30px;
	float: right;
	margin-right: 30px;
	font-weight: bold;
	line-height: 35px;
	font-size: 18px;
}
.datebox {
	margin-top: 10px;
	width: 100%;
	display: flex;
	flex-direction: row;
	justify-content: space-around;
}
.column {
	width: 35px;
	height: 65px;
	border-radius: 10px;
	display: flex;
	flex-direction: column;
	justify-content: inherit;
	align-items: center;
}
.iscolumn {
	background-color: #7364e3;
	color: #FFFFFF;
}
.jinbox {
	display: flex;
	flex-direction: row;
	justify-content: space-around ;
}
.jin {
	background-color: #7364e3;
	border-radius: 100%;
	margin-top: 3px;
	color: #FFFFFF;
	font-size: 12px;
	border: 3px solid #7364e3;
	opacity: 0;
}
.isjin {
	opacity: 1;
}
.listbox {
	display: flex;
	flex-direction: column;
}
.linebox {
	margin-top: 20px;
	width: 100%;
	height: 340rpx;
	display: flex;
	flex-direction: row;
	justify-content: space-around;
}
.box {
	width: 330rpx;
	height: 325rpx;
	background-color: #ffffff;
	border-radius: 20px;
}
.classname {
	color: #1c1b1b;
	font-size: 28px;
	margin: 40rpx 0 20rpx 40rpx;
	font-weight: bold;
}
.nums {
	font-size: 14px;
	color: #7c7c7c;
	margin-left: 40rpx;
}
.itemlist {
	margin-top: 90rpx;
	display: flex;
	flex-direction: row;
	justify-content: space-around;
}
.listone {
	font-size: 12px;
	height: 40rpx;
	width: 120rpx;
	background-color: #E8F7F3;
	color: #4ECA9D;
	text-align: center;
	line-height: 20px;
	border-radius: 12px;
}
.listtwo {
	font-size: 12px;
	height: 40rpx;
	width: 120rpx;
	background-color: #FFF1F3;
	color: #FE8C9F;
	text-align: center;
	line-height: 20px;
	border-radius: 12px;
}

